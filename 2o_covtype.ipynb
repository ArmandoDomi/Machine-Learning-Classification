        "import numpy as np\n",
        "import sklearn as sk\n",
        "\n",
        "from sklearn import utils\n",
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "from sklearn.svm import SVC\n",
        "from sklearn import metrics\n",
        "from sklearn.model_selection import KFold\n",
        "from sklearn.preprocessing import MinMaxScaler\n",
        "from sklearn.model_selection import GridSearchCV\n",
        "\n",
        "def printing_Kfold_scores(X,y):\n",
        "\n",
        "  all_acc=0\n",
        "  \n",
        "  # Different C and gamma parameters\n",
        "  C_values = [0.1, 1, 10]\n",
        "  gamma_values = [0.01, 0.1, 1]\n",
        "  \n",
        "  best_acc=0\n",
        "  bestParam={}\n",
        "  for C in C_values:\n",
        "    \n",
        "    for gamma in gamma_values:\n",
        "        all_acc=0\n",
        "        print('-------------------------------------------')\n",
        "        print('C parameter: \\n', C)\n",
        "        print('gamma parameter: \\n', gamma)\n",
        "        print('-------------------------------------------')\n",
        "        print('')\n",
        "\n",
        "        rbf_svm =SVC(C=C, kernel='rbf', gamma=gamma)\n",
        "        cv = KFold(n_splits=10, shuffle=False)\n",
        "\n",
        "        for train_index, test_index in cv.split(X):\n",
        "\n",
        "          X_train, X_test, y_train, y_test = X[train_index], X[test_index], y[train_index], y[test_index]\n",
        "          rbf_svm.fit(X_train, y_train)\n",
        "          y_pred = rbf_svm.predict(X_test)\n",
        "          acc = metrics.accuracy_score(y_test, y_pred)\n",
        "          all_acc=acc+all_acc\n",
        "          print(\"RBF SVM accuracy: %f\" % (acc))\n",
        "\n",
        "        # find the best parameters \n",
        "        m_acc=(all_acc/10)\n",
        "        if m_acc > best_acc:\n",
        "          best_acc=m_acc\n",
        "          bestParam={'C':C,'gamma':gamma}\n",
        "        print(\"Mean RBF SVM accuracy: %f\" %(m_acc))\n",
        "\n",
        "\n",
        "  return bestParam\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        #put the dataset path
        "data=pd.read_csv(\"\",delimiter=',',header=None).values;\n",
        "#print the first 10 rows of the dataset\n",
        "#The shape of the variable data is (581012,55) \n",
        "\n",
        "# let's separate the data from the targets. \n",
        "X=data[:,:53]\n",
        "y=data[:,54]\n",
        "\n",
        "#we will take the first 15120 rows for training\n",
        "train_id=15120\n",
        "X_train = X[0:train_id, :]\n",
        "X_test = X[train_id:, :]\n",
        "y_train = y[0:train_id]\n",
        "y_test = y[train_id:]\n",
        "\n",
        "scaler = MinMaxScaler(feature_range=(0.0, 1.0))\n",
        "scaler.fit(X_train)\n",
        "X_train = scaler.transform(X_train)\n",
        "X_test = scaler.transform(X_test)\n",
        "\n",
        "# we get the best parameters \n",
        "bestParam=printing_Kfold_scores(X_train,y_train)\n",
        "\n",
        "# now let's use our best parameters to our model\n",
        "rbf_svm =SVC(C=bestParam['C'], kernel='rbf', gamma=bestParam['gamma'],decision_function_shape='ovo')\n",
        "rbf_svm.fit(X_train, y_train)\n",
        "\n",
        "ypred = rbf_svm.predict(X_test)\n",
        "acc = metrics.accuracy_score(y_test, ypred)\n",
        "print()\n",
        "print(\"At test set : \\n\")\n",
        "print(\"RBF SVM accuracy: %f\" % (acc))\n",
        "\n",
        "#let's print the support vectors of the model\n",
        "print(\"Support Vectors : \\n\")\n",
        "print(rbf_svm.support_vectors_.shape)\n",
        "print(rbf_svm.support_vectors_)\n",
        "\n",
        "\n",
        "# using the ovo scheme there are n*(n-1)/2=21 classifiers (n=7 is the number of classes)\n",
        "\n",
        "rbf_ovo = rbf_svm.decision_function(X_test)\n",
        "print(rbf_ovo.shape)\n"
      ],
